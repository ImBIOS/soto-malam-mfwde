{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console":"off",
        "quotes": [2, "double", { "avoidEscape": true }],
        "no-underscore-dangle":"off",
        "no-restricted-globals":"off",
        "consistent-return": 0,
        "no-prototype-builtins": 0
    },
    "plugins": ["jasmine"]
}
